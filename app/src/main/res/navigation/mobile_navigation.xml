<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_feeds">

    <fragment
        android:id="@+id/navigation_feeds"
        android:name="com.example.finalproject.ui.feeds.FeedsFragment"
        android:label="@string/feeds"
        tools:layout="@layout/fragment_feeds" />

    <fragment
        android:id="@+id/navigation_community"
        android:name="com.example.finalproject.ui.community.CommunityFragment"
        android:label="@string/community"
        tools:layout="@layout/fragment_community" />

    <fragment
        android:id="@+id/navigation_chats"
        android:name="com.example.finalproject.ui.chats.ChatsFragment"
        android:label="@string/chats"
        tools:layout="@layout/fragment_chats" />

    <fragment
        android:id="@+id/navigation_account"
        android:name="com.example.finalproject.ui.account.AccountFragment"
        android:label="@string/account"
        tools:layout="@layout/fragment_account" />
</navigation>